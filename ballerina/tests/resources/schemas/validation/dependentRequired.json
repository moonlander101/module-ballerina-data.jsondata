{
  "schema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "credit_card": {"type": "string"},
      "billing_address": {"type": "string"}
    },
    "dependentRequired": {
      "credit_card": ["billing_address"]
    }
  },
  "valid": [
    {"credit_card": "1234", "billing_address": "123 Main St"}
  ],
  "invalid": [
    {"credit_card": "1234"}
  ]
}
